{
  "version": 2,

  "functions": {
    "amrdevs/wsgi.py": {
      "runtime": "python3.11",
      "maxDuration": 60,
      "memory": 1024
    }
  },

  "builds": [
    {
      "src": "amrdevs/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": ".vercel/output" }
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "amrdevs/wsgi.py"
    }
  ]
}
