{
  
  "buildCommand": "bash build.sh",

  "builds": [
	      {
        "src": "amrdevs/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.11", "maxLambdaSize": "50mb", "maxDuration": 60 }
	      }
	    ],
  "routes": [
	      {
        "src": "/(.*)",
        "dest": "/amrdevs/wsgi.py"
	      }
	    ]
	  }