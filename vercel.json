{
  "builds": [
    {
      "src": "amrdevs/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11", "maxLambdaSize": "50mb" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": ".vercel/output" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/amrdevs/wsgi.py"
    }
  ]

  ,
  "functions": {
    "amrdevs/wsgi.py": {
      "maxDuration": 60
    }
  }
}